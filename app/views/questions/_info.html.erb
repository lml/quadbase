<% if !question.number.nil? %>
<div class="quad-quid">
q. <%= question.number %>, 

<% if question.version.nil? %>
   unpublished 
<% else %>
   v. <%= question.version %>
   <% if question.is_latest? %>
      (LATEST)
   <% end %>
   <% if question.is_embargoed? %>
     <br/>
     Embargoed
     <%= " until " + (question.embargoed_until).to_s unless question.publisher.is_privileged? %>
   <% end %>
<% end %>
</div>
<% end %>
