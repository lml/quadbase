<%= form_tag :url => 'import/qti', :method => 'post', :multipart => true, do %>
	<%= label_tag "File", "File" %>
	<br>
	<%= file_field_tag :file %>
	<br>
	<br>
	<%= label_tag "name", "Content Type" %>
	<br>
	<%= select_tag :content_type, options_for_select(@content_types).html_safe, :include_blank => true %>
	<br>
	<br>
	<%= submit_tag "Import" %>
<% end %>