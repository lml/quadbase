<%= display_flash %>

<% @list_questions.each do |wq| %>
  $("<%= "#question_" + wq.question.id.to_s %>").remove();
<% end %>

<% if @list.list_questions.empty? %>
  <%= show_none %>
    $("#publish_button").remove();
    $("#move_menu").remove();
    $("#copy_menu").remove();
    $("#remove_button").remove();
<% end %>
